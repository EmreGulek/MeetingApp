<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - MeetingApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.min.css" integrity="sha512-jnSuA4Ss2PkkikSOLtYs8BlYIeeIK1h99ty4YfvRPAlzr377vr3CXDb7sb7eEEBYjDtcYj+AjBH3FLv5uSJuXg==" crossorigin="anonymous" referrerpolicy="no-referrer" />    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/MeetingApp.styles.css" asp-append-version="true"/>

</head>
<body>

<div class="bg-danger text-white p-2 text-center">
    MeetingApp
</div>
<div class="container mt-3">
    <div class="row">
        <div class="col-lg-6 mx-auto text-center">
            <div class="card">
                <div class="card-body">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
</div>

</body>
</html>